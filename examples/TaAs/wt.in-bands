&TB_FILE
Hrfile = "Wannier90_hr.dat"
Package='VASP'
/


!> bulk band structure calculation flag
&CONTROL
BulkBand_calc         = T
/

&SYSTEM
NumOccupied = 16        ! NumOccupied
SOC = 1                 ! without soc : SOC=0; with soc : SOC=1
